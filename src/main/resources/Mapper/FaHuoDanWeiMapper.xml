<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znczLfyl.dao.FaHuoDanWeiMapper">
	<select id="queryForInt" resultType="java.lang.Integer">
		select count(*) from yun_shu_shang where 1=1
		<if test="mc!=''&amp;mc!=null">
			and mc like CONCAT('%',#{mc},'%')
		</if> 
	</select>
	<select id="queryList" resultType="com.znczLfyl.entity.FaHuoDanWei">
		select * from yun_shu_shang where 1=1
		<if test="mc!=''&amp;mc!=null">
			and mc like CONCAT('%',#{mc},'%')
		</if> 
	</select>
</mapper>