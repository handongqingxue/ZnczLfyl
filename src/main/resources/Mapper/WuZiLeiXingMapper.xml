<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.znczLfyl.dao.WuZiLeiXingMapper">
	<insert id="add" parameterType="com.znczLfyl.entity.WuZiLeiXing">
		insert into wu_zi_lei_xing
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="mc!=null">
				mc,
			</if>
				cjsj,
			<if test="px!=null">
				px,
			</if>
			<if test="bz!=null">
				bz,
			</if>
		</trim>
		values
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="mc!=null">
				#{mc},
			</if>
				now(),
			<if test="px!=null">
				#{px},
			</if>
			<if test="bz!=null">
				#{bz},
			</if>
		</trim>
	</insert>
	<select id="queryForInt" resultType="java.lang.Integer">
		select count(*) from wu_zi_lei_xing where 1=1
		<if test="mc!=''&amp;mc!=null">
			and mc like CONCAT('%',#{mc},'%')
		</if>
	</select>
	<select id="queryList" resultType="com.znczLfyl.entity.WuZiLeiXing">
		select * from wu_zi_lei_xing where 1=1
		<if test="mc!=''&amp;mc!=null">
			and mc like CONCAT('%',#{mc},'%')
		</if>
		order by px
		limit #{rowNum},#{rows}
	</select>
</mapper>